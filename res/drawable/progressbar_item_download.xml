<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:innerRadiusRatio="3.5" android:thicknessRatio="8.0" android:useLevel="true">
            <gradient android:startColor="@color/ck_e4e7ec" android:endColor="@color/ck_e4e7ec" android:angle="0.0" android:type="sweep" android:centerColor="@color/ck_e4e7ec" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="3.5" android:thicknessRatio="8.0" android:useLevel="true">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%" />
                <gradient android:startColor="@color/ck_98a2b3" android:endColor="@color/ck_98a2b3" android:angle="0.0" android:type="sweep" android:centerColor="@color/ck_98a2b3" />
            </shape>
        </rotate>
    </item>
</layer-list>